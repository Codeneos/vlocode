{
    "private": true,
    "name": "vlocode-project",
    "packageManager": "yarn@3.2.0",
    "description": "Vlocode project packages",
    "version": "0.16.30",
    "license": "MIT",
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "publisher": "curlybracket",
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "categories": [
        "Other"
    ],
    "repository": "https://github.com/codeneos/vlocode.git",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues",
        "email": "peter@curlybracket.nl"
    },
    "devDependencies": {
        "@types/fs-extra": "^9",
        "@types/jest": "^28.1.3",
        "@types/node": "^16",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.5",
        "chalk": "^4.1.2",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^37.6.1",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.0.0",
        "fs-extra": "^9",
        "jest": "^28.1.1",
        "js-yaml": "^4.1.0",
        "log-symbols": "^4.1.0",
        "prettier": ">=2.0.0",
        "ts-jest": "^28.0.5",
        "typescript": "4.6.4"
    },
    "pnpm": {
        "patchedDependencies": {
            "jsforce@1.11.0": "patches/jsforce@1.11.0.patch",
            "fast-glob@3.2.11": "patches/fast-glob@3.2.11.patch",
            "@salesforce/core@2.33.1": "patches/@salesforce__core@2.33.1.patch",
            "@oclif/command@1.8.16": "patches/@oclif__command@1.8.16.patch",
            "typescript@4.6.4": "patches/typescript@4.6.4.patch",
            "vlocity@1.14.18": "patches/vlocity@1.14.18.patch",
            "@types/jsforce@1.9.42": "patches/@types__jsforce@1.9.42.patch"
        }
    },
    "scripts": {
        "build": "pnpm run -r build",
        "watch": "pnpm run -r --parallel --stream --no-bail watch",
        "watch-cli": "pnpm run --filter @vlocode/cli... --parallel --stream --no-bail watch",
        "build-cli": "pnpm run --filter @vlocode/cli... build",
        "pack-cli": "pnpm run --filter @vlocode/cli^... build && pnpm run --filter @vlocode/cli pack",
        "pack-extension": "pnpm run --filter vlocode^... build && pnpm run --filter vlocode pack",
        "prepublish": "pnpm run --filter @vlocode/* build",
        "publish": "lerna publish from-package --no-git-reset",
        "test": "jest",
        "lint": "eslint ./packages/*/src --ext .ts",
        "docs": "typedoc",
        "bump": "ts-node ./build/bumpVersion.ts patch"
    },
    "resolutions": {
        "@types/fs-extra": "^9",
        "@oclif/command": "^1.8",
        "@types/node": "^16",
        "@salesforce/core": "2.33.1",
        "fs-extra": "9",
        "js-force": "1.11.0",
        "@types/jsforce": "1.9.42",
        "chalk": "^4.1.2",
        "js-yaml": "^4.1.0",
        "typescript": "4.6.4"
    },
    "dependencies": {
        "@lerna-lite/cli": "^1.6.0"
    }
}
