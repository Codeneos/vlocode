{
    "name": "@vlocode/util",
    "version": "0.16.21",
    "description": "Vlocode utility library",
    "main": "lib/index.js",
    "readme": "../SITE.md",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "watch": "yarn pre-build && tsc -w & tsc-alias -w",
        "build": "yarn pre-build && tsc && tsc-alias",
        "pack": "yarn build && yarn pack --out %s-%v.tgz",
        "pre-build": "cti -w -b ./src -i constants",
        "postinstall": "patch-package"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Codeneos/vlocode.git"
    },
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "files": [
        "lib/**/*.d.ts",
        "lib/**/*.js",
        "patches/*.patch",
        "../SITE.md"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues"
    },
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "devDependencies": {
        "@types/fs-extra": "^9",
        "@types/node": "^16",
        "@types/vscode": "^1.61.0",
        "create-ts-index": "^1.14.0",
        "tsc-alias": "1.6.7",
        "typescript": "4.6.4"
    },
    "dependencies": {
        "@milahu/patch-package": "^6.4.11",
        "@salesforce/core": "2.33.1",
        "fast-xml-parser": "^3.19.0",
        "fs-extra": "^9.0",
        "he": "^1.2.0",
        "open": "^8.2.1",
        "optional-require": "^1.1.7",
        "reflect-metadata": "^0.1.13",
        "uuid": "^8.3.2"
    },
    "publisher": "curlybracket"
}
