{
    "name": "@vlocode/util",
    "version": "0.16.22",
    "description": "Vlocode utility library",
    "main": "lib/index.js",
    "readme": "../SITE.md",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": ">=12.0.0",
        "npm": ">=6.0.0"
    },
    "scripts": {
        "watch": "tsc -w & tsc-alias -w",
        "build": "tsc && tsc-alias",
        "prebuild": "cti -w -b ./src -i constants",
        "postinstall": "npx patch-package"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Codeneos/vlocode.git"
    },
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "files": [
        "lib/**/*.d.ts",
        "lib/**/*.js",
        "patches/*.patch",
        "../SITE.md"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues"
    },
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "devDependencies": {
        "create-ts-index": "^1.14.0",
        "typescript": "^4.5.4",
        "tsc-alias": "1.6.7"
    },
    "dependencies": {
        "@salesforce/core": "^2.28.2",
        "fast-xml-parser": "^3.19.0",
        "fs-extra": "^10.0.0",
        "he": "^1.2.0",
        "open": "^8.2.1",
        "optional-require": "^1.1.7",
        "reflect-metadata": "^0.1.13",
        "uuid": "^8.3.2",
        "patch-package": "^6.4.7"
    },
    "publisher": "curlybracket"
}
