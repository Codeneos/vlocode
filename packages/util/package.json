{
    "name": "@vlocode/util",
    "version": "0.18.7",
    "description": "Vlocode utility library",
    "main": "lib/index.js",
    "readme": "../SITE.md",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "clean": "shx rm -rf ./lib ./coverage ./tsconfig.tsbuildinfo",
        "build": "pnpm run pre-build && tsc && tsc-alias",
        "watch": "pnpm run pre-build && tsc -w & tsc-alias -w",
        "pack": "pnpm run build && pnpm pack",
        "prepublish": "pnpm run build",
        "pre-build": "cti -w -b ./src -i constants",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Codeneos/vlocode.git"
    },
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "files": [
        "lib/**/*.d.ts",
        "lib/**/*.js",
        "patches/*.patch",
        "../SITE.md"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues"
    },
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "devDependencies": {
        "@types/fs-extra": "^9",
        "@types/jest": "^28.1.6",
        "@types/node": "^16",
        "@types/vscode": "^1.70.0",
        "create-ts-index": "^1.14.0",
        "jest": "^28.1.3",
        "shx": "^0.3.4",
        "ts-jest": "^28.0.5",
        "tsc-alias": "1.6.7",
        "typescript": "4.6.4"
    },
    "dependencies": {
