{    
    "extends": "../../tsconfig",
    "compilerOptions": {
        "outDir": "out",
        "sourceMap": true,
        "baseUrl": ".",
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@root/*": ["src/*"],
            "@constants": ["src/constants"],
            "@util/*": ["src/lib/util/*"],
            "@lib/*": ["src/lib/*"],
            "@logging/*": ["src/lib/logging/*"],
            "@events/*": ["src/events/*"],
            "@commands/*": ["src/commands/*"]
        },
        "types": [
            "node",
            "webpack-env"
        ]
    },
    "include": [ "./src/**/*", "./weboack.*" ]
}