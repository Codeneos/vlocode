{
    "name": "@vlocode/core",
    "version": "0.19.16",
    "description": "Vlocode core IoC container framework library",
    "main": "lib/index.js",
    "readme": "../SITE.md",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": ">=12.0.0",
        "npm": ">=6.0.0"
    },
    "scripts": {
        "build": "pnpm run pre-build && tsc",
        "clean": "shx rm -rf ./lib ./coverage ./tsconfig.tsbuildinfo ./src/**/*.d.ts ./src/**/*.d.ts.map",
        "watch": "pnpm run pre-build && tsc -w",
        "pack": "pnpm run build && pnpm pack",
        "prepublish": "pnpm run build",
        "pre-build": "cti -w -b ./src/fs ./src/logging/writers"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Codeneos/vlocode.git"
    },
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "files": [
        "lib/**/*.d.ts",
        "lib/**/*.js",
        "patches/*.patch",
        "../SITE.md"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues"
    },
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "devDependencies": {
        "@types/fs-extra": "^9",
        "@types/jest": "^28.1.6",
        "@types/luxon": "^3.1.0",
        "@types/node": "^20.4.2",
        "create-ts-index": "^1.14.0",
        "jest": "^29.6.1",
        "shx": "^0.3.4",
        "ts-jest": "^29.1.1",
        "typescript": "5.1.6"
    },
    "dependencies": {
        "@vlocode/util": "workspace:*",
        "chalk": "^4.1.1",
        "globby": "^11.0.4",
        "luxon": "^3.1.0",
        "memfs": "^3.2.2",
        "minimatch": "^9.0.3",
        "moment": "^2.29.1",
        "reflect-metadata": "^0.1.13",
        "unique-names-generator": "^4.6.0"
    },
    "publisher": "curlybracket",
    "gitHead": "f16fa9dcbf7ef17328047ecb07ab4a7e6a94140a"
}
