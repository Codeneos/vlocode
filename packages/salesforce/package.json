{
    "name": "@vlocode/salesforce",
    "version": "0.16.21",
    "description": "Salesforce utility library for Vlocode",
    "keywords": [
        "Salesforce"
    ],
    "main": "lib/index.js",
    "readme": "../SITE.md",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "watch": "yarn pre-build && tsc -w & tsc-alias -w",
        "build": "yarn pre-build && tsc && tsc-alias",
        "pre-build": "cti -w -b ./src -i constants",
        "postinstall": "patch-package"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Codeneos/vlocode.git"
    },
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "files": [
        "lib/**/*.d.ts",
        "lib/**/*.js",
        "patches/*.patch",
        "../SITE.md"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues"
    },
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "devDependencies": {
        "@types/fs-extra": "^9",
        "@types/jsforce": "^1.9.41",
        "@types/node": "^16",
        "create-ts-index": "^1.14.0",
        "tsc-alias": "^1.6.7",
        "webpack-env": "^0.8.0"
    },
    "dependencies": {
        "@milahu/patch-package": "^6.4.11",
        "@salesforce/source-deploy-retrieve": "^5.12.11",
        "@vlocode/core": "workspace:*",
        "@vlocode/util": "workspace:*",
        "axios": "^0.25.0",
        "chalk": "^4.1.1",
        "fs-extra": "^9.0",
        "glob": "^7.1.7",
        "js-yaml": "^4.1.0",
        "jsforce": "^1.11.0",
        "jszip": "^3.7.0",
        "log-symbols": "^4.0.0",
        "moment": "^2.29.1",
        "salesforce-alm": "^52.1.9",
        "xml2js": "^0.4.23"
    },
    "publisher": "curlybracket"
}
