{
    "name": "@vlocode/cli",
    "version": "0.16.32",
    "description": "Hyper fast Salesforce/Vlocity datapack deployment CLI",
    "keywords": [
        "Vlocity",
        "Salesforce",
        "Datapacks",
        "Deployment"
    ],
    "main": "lib/index.js",
    "bin": {
        "vlocode": "bin/run"
    },
    "publishConfig": {
        "main": "lib/index.js",
        "typings": "lib/index.d.ts"
    },
    "readme": "./README.md",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=6.0.0"
    },
    "scripts": {
        "watch": "tsc -w",
        "watch-webpack": "webpack --watch --config webpack/webpack.ts",
        "build": "tsc -b",
        "build-webpack": "webpack --config webpack/webpack.ts",
        "pack": "pnpm run prepublish && pnpm run pack-only",
        "prepublish": "pnpm run clean && pnpm run build-webpack && tsc",
        "pack-only": "pnpm pack",
        "clean": "rimraf ./dist ./lib ./.ts-temp"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Codeneos/vlocode.git"
    },
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "files": [
        "/lib",
        "/dist",
        "/bin"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues"
    },
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "devDependencies": {
        "@types/fs-extra": "^9",
        "@types/jest": "^28.1.3",
        "@types/node": "^16",
        "@vlocode/core": "^0.16.32",
        "@vlocode/salesforce": "^0.16.32",
        "@vlocode/util": "^0.16.32",
        "@vlocode/vlocity-deploy": "^0.16.32",
        "chalk": "^4.1.1",
        "commander": "^9.2.0",
        "create-ts-index": "^1.14.0",
        "fs-extra": "^9.0",
        "glob": "^7.1.7",
        "jest": "^28.1.1",
        "json-loader": "^0.5.7",
        "log-symbols": "^4.0.0",
        "moment": "^2.29.1",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.21",
        "ts-jest": "^28.0.5",
        "ts-loader": "^9.2.6",
        "tsc-alias": "^1.6.7",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "4.6.4",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "webpack-env": "^0.8.0",
        "webpack-glob-entry": "^2.1.1",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "publisher": "curlybracket",
    "gitHead": "53b3159d6ea2c9112996a8b7bd2dd67bfb0efe62"
}
