{
    "name": "@vlocode/cli",
    "version": "0.16.21",
    "description": "Hyper fast Salesforce/Vlocity datapack deployment CLI",
    "keywords": [
        "Vlocity",
        "Salesforce",
        "Datapacks",
        "Deployment"
    ],
    "main": "lib/index.js",
    "bin": {
        "vlocode": "lib/index.js"
    },
    "publishConfig": {
        "main": "lib/index.js",
        "typings": "lib/index.d.ts"
    },
    "readme": "../SITE.md",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": ">=12.0.0",
        "npm": ">=6.0.0"
    },
    "scripts": {
        "watch": "yarn pre-build && webpack --watch --config webpack/webpack.ts",
        "build": "yarn pre-build && webpack --config webpack/webpack.ts",
        "pack": "yarn build && yarn pack --out %s-%v.tgz",
        "pre-build": "cti -w -b ./src/commands"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Codeneos/vlocode.git"
    },
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "files": [
        "lib/**/*.d.ts",
        "lib/**/*.js"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues"
    },
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "devDependencies": {
        "@types/fs-extra": "^9",
        "@types/node": "^16",
        "create-ts-index": "^1.14.0",
        "json-loader": "^0.5.7",
        "ts-loader": "^9.2.6",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "tsc-alias": "^1.6.7",
        "typescript": "4.6.4",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "webpack-env": "^0.8.0",
        "webpack-glob-entry": "^2.1.1",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "dependencies": {
        "@vlocode/core": "workspace:*",
        "@vlocode/salesforce": "workspace:*",
        "@vlocode/util": "workspace:*",
        "@vlocode/vlocity-deploy": "workspace:*",
        "chalk": "^4.1.1",
        "commander": "^9.2.0",
        "fs-extra": "^9.0",
        "glob": "^7.1.7",
        "log-symbols": "^4.0.0",
        "moment": "^2.29.1"
    },
    "publisher": "curlybracket"
}
