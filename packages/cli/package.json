{
    "name": "@vlocode/cli",
    "version": "1.40.0-beta-1",
    "description": "Hyper fast Salesforce/Vlocity datapack deployment CLI",
    "keywords": [
        "Vlocity",
        "Salesforce",
        "Datapacks",
        "Deployment"
    ],
    "bin": {
        "vlocode": "bin/run"
    },
    "main": "src/index.ts",
    "type": "module",
    "publishConfig": {
        "main": "dist/cli.mjs"
    },
    "readme": "./README.md",
    "directories": {
        "lib": "./lib"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "watch": "tsdown -c tsdown.config.ts --watch",
        "build": "tsdown -c tsdown.config.ts",
        "prepublish": "pnpm run clean && pnpm run build",
        "pack-only": "pnpm pack",
        "clean": "shx rm -rf ./dist ./lib ./.ts-temp './*.tgz' './src/**/*.{d.ts,ts.map,js.map,js}'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Codeneos/vlocode.git"
    },
    "author": {
        "name": "Peter van Gulik",
        "email": "peter@curlybracket.nl"
    },
    "files": [
        "/lib",
        "/dist",
        "/bin"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Codeneos/vlocode/issues"
    },
    "homepage": "https://github.com/Codeneos/vlocode#readme",
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@types/js-yaml": "^4.0.2",
        "@vlocode/apex": "workspace:*",
        "@vlocode/core": "workspace:*",
        "@vlocode/omniscript": "workspace:*",
        "@vlocode/salesforce": "workspace:*",
        "@vlocode/util": "workspace:*",
        "@vlocode/vlocity": "workspace:*",
        "@vlocode/vlocity-deploy": "workspace:*",
        "chalk": "^4.1.2",
        "commander": "^11.1.0",
        "esbuild": "^0.25.4",
        "esbuild-loader": "^4.3.0",
        "glob": "^11.0.2",
        "jest": "^29.7.0",
        "js-yaml": "^4.1.0",
        "log-symbols": "^4.0.0",
        "shx": "^0.3.4",
        "ts-jest": "^29.3.4",
        "ts-loader": "^9.5.2",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "webpack": "^5.99.9",
        "webpack-cli": "^6.0.1",
        "webpack-merge": "^6.0.1"
    },
    "publisher": "curlybracket"
}
